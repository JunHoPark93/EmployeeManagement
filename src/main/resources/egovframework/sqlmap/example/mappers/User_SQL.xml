<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="egovframework.controller.login.service.impl.LoginMapper">

	<select id="login" resultType="egovframework.controller.login.service.impl.UserVO">
		SELECT * FROM 
		
			T_USER
			
			WHERE ID = #{id} and PW = #{pw}
		
	</select>
	

</mapper>